package com.techlabs.behavioural.state.model;

public class Packet {

	private IPacketState state;
	
	
	
	public Packet(IPacketState state) {
		super();
		this.state = new Ordered();
	}

	public IPacketState getState() {
        return state;
    }

    public void setState(IPacketState state) {
        this.state = state;
    }
	
	private void nextState() {
		// TODO Auto-generated method stub
		state.next(this);
	}

	public void previousState() {
		state.previous(this);
	}
	
	public void printCurrentState(IPacketState Packet) {
		System.out.println("State of Your Packet is at : "+Packet);
	}
	
}
