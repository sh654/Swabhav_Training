	public static void main(String[] args) {
		
		ICar car = new Maruti();
		car.start();
		car.stop();
		
		ICar car2 = new Tata();
		car2.start();
		car2.stop();
		
		ICar car3 = new Mahindra();
		car3.start();
		car3.stop();
		
	}
}
