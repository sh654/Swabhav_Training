package com.techlabs.behavioural.model;

public class AppNotifier implements INotifier{

	@Override
	public void notify(Account accounts, String message) {
		// TODO Auto-generated method stub
		System.out.println("App Notification to : "+accounts.getAccountNumber() + "Balance Debited"+ accounts.getBalance() +" Current Balance: "+accounts.getBalance());
	}
	

}
