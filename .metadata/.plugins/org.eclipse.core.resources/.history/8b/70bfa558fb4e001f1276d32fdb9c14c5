package com.techlabs.creational.singelton.model;

public class Login {
	
	private static Login login;
	
	private Login() {
		System.out.println("Login Created");
	}
	
	public static getLogin() {
		if(login == null)
			login = new Login();
		return login;
	}
	
	public void status() {
		System.out.println("Login Done");
	}
	
}
