package com.techlabs.behavioural.state.model;

public class Packet {

	private IPacketState Packet;
	
	
	
	public Packet(IPacketState packet) {
		super();
		Packet = packet;
	}

	public void nextState(IPacketState Packet) {
		Packet.next(Packet);
	}
	
	public void previousState(IPacketState Packet) {
		Packet.previous(Packet);
	}
	
	public void printCurrentState(IPacketState Packet) {
		System.out.println("State of Your Packet is at : "+Packet);
	}
	
}
