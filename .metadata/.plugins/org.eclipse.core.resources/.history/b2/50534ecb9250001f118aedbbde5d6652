package com.techlabs.behavioural.model;

public class SMSNotifiers implements INotifier {

	@Override
	public void notify(Account accounts, String message) {
		// TODO Auto-generated method stub
		System.out.println("SMS Notification to : "+accounts.getAccountNumber() + "Balance Debited"+ accounts.getBalance() +" Current Balance: "+accounts.getBalance());
	}


}
