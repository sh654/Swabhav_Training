package com.techlabs.ocp.model;

import com.techlabs.ocp.solution.Shape;

public class Rectangle implements Shape {
	
	private int width;
	private int height;
	
	public Rectangle(int width, int height) {
		super();
		this.width = width;
		this.height = height;
	}
	
	public int getArea() {
		return width * height;
	}
}
